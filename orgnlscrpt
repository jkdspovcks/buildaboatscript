local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

local Window = Library.CreateLib("//NEW// OP Build A Boat For Treasure I Made by flexsz#7677 ", "Synapse")
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Auto-Farm")

MainSection:NewButton("Auto Farm 1 {Flexsz Auto Farm}", "Option1", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/jkdspovcks/df/main/sddds"))()
end)


MainSection:NewButton("Auto Farm 2", "Option2", function()
loadstring(game:HttpGet(('https://raw.githubusercontent.com/JustAP1ayer/BABFTPLAYERHUB/main/BABFTV2.lua'),true))()
end)

MainSection:NewButton("Auto Farm 3", "Option3", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/XRoLLu/UWU/main/BUILD%20A%20BOAT%20FOR%20TREASURE.lua'))()
end)

MainSection:NewButton("Auto Farm 4", "Option4", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/CasperFlyModz/discord.gg-rips/main/BuildABoatForTreasure.lua"))()
end)

local MainSection = Main:NewSection("Auto-Build")
MainSection:NewButton("Auto Build Old", "Option1", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/max2007killer/auto-build-not-limit/main/autobuild.txt"))()
end)
MainSection:NewButton("Auto Build New", "Option2", function()
loadstring(((syn and syn.request) or (fluxus and fluxus.request) or request or http_request)({Url = "https://www.stenutilities.com/babft"})["Body"])()
end)


local Misc = Window:NewTab("Misc")

local MiscSection = Misc:NewSection("Player")

MiscSection:NewSlider("Speed", "Walkspeed", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)

MiscSection:NewSlider("Jump", "JumpPower", 500, 0, function(s) -- 500 (MaxValue) | 0 (MinValue)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower= s
end)

local Credit = Window:NewTab("Credit")

local CreditSection = Credit:NewSection("By flexsz#7677")

CreditSection:NewButton("Click For Discord Server", "https://discord.com/invite/ZKFEzk3JbE", function()
game:GetService("StarterGui"):SetCore("SendNotification", {Title = "Copied!"; Text = "PASTE IT ON SEARCH !"; Icon = "http://www.roblox.com/asset/?id=9597859601"; Duration = 10})
	wait(0.7)
	setclipboard("https://discord.com/invite/ZKFEzk3JbE")
end)
